{
  "name": "DonnieWestBlog",
  "description": "My Blog",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "eslint 'src/**/*.js'",
    "format": "prettier-eslint --write 'src/**/*.js'"
