{
  "name": "ReactBlog",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-preset-react-app": "^3.1.0",
    "change-case": "^3.0.1",
    "concurrently": "^3.5.1",
    "eslint": "^4.13.0",
    "eslint-config-koality": "^2.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-jsx": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "excerpt-html": "^1.2.0",
    "git-directory-deploy": "^1.5.1",
    "gray-matter": "^3.0.3",
    "highlightjs": "^9.10.0",
    "markdown-it": "^8.4.0",
    "markdown-it-footnote": "^3.0.1",
    "nodemon": "^1.12.5",
    "prep": "^1.6.2",
    "prettier": "^1.9.1",
    "prettier-eslint": "^8.2.5",
    "prettier-eslint-cli": "4.7.1",
    "react-app-rewired": "^1.3.6",
    "react-scripts": "1.1.1",
    "react-snap": "^1.11.5",
    "rss": "^1.2.2",
    "serve": "^6.4.1",
    "workbox-cli": "^3.0.1"
  },
  "dependencies": {
    "preact": "^8.2.6",
    "preact-compat": "^3.17.0",
    "react-router-dom": "4.2.2",
    "rrc": "^0.10.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "koality",
    "globals": [
      "fetch",
      "it"
    ],
    "env": {
      "browser": true,
      "node": true
    }
  },
  "reactSnap": {
    "source": "build"
  },
  "scripts": {
    "start": "npm run compile-posts && react-app-rewired start",
    "dev": "concurrently \"react-app-rewired start\" \"npm run watch\"",
    "now-start": "serve ./build --single",
    "build": "npm run compile-posts && react-app-rewired build && workbox generateSW",
    "postbuild": "react-snap",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build && git-directory-deploy --directory build --branch master",
    "watch": "nodemon -w posts -e markdown compileMarkdown.js",
    "lint": "eslint \"src/**/*.js\"",
    "format": "prettier-eslint --write \"src/**/*.js\"",
    "compile-posts": "node ./compileMarkdown.js"
  }
}
